{% block form_errors -%}
    {% if errors|length > 0 -%}
        {% if form.parent %}<div class="mar10 alert alert-danger alert-dismissible">{% else %}<div class="mar10 alert alert-danger alert-dismissible">{% endif %}
        {%- for error in errors -%}
            <span class="glyphicon glyphicon-exclamation-sign"></span> {{ error.message }}
        {%- endfor -%}
        {% if form.parent %}</div>{% else %}</div>{% endif %}
    {%- endif %}
{%- endblock form_errors %}